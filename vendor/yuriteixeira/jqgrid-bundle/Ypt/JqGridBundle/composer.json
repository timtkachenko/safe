{
    "name": "yuriteixeira/jqgrid-bundle",
    "type": "symfony-bundle",
    "description": "An improved JqGrid integration for Symfony2, focusing on a more readable code and distributing an updated version of jquery plugin (based on michelpa/JqGridBundle)",
    "keywords": ["datagrid","jqgrid"],
    "homepage": "http://github.com/yuriteixeira/JqGridBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuri Teixeira",
            "email": "oyuriteixeira@gmail.com",
            "homepage": "http://www.yuriteixeira.com.br"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "2.*",
        "doctrine/orm": "2.*",
        "knplabs/knp-paginator-bundle": "2.*"
    },
     "autoload": {
        "psr-0": { "Ypt\\JqGridBundle": "" }
    },
    "target-dir": "Ypt/JqGridBundle"
}
